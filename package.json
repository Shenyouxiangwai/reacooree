{
  "name": "reacooree",
  "displayName": "Reacooree",
  "description": "",
  "version": "0.0.1",
  "publisher": "Shenyouxiangwai",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shenyouxiangwai/reacooree.git"
  },
  "icon": "images/IMG_4094.jpg",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "reacooree.分析文件内的 hooks 结构",
        "title": "分析文件内的 hooks 结构",
        "category": "analyze"
      },
      {
        "command": "reacooree.分析组件内的 hooks 结构",
        "title": "分析组件内的 hooks 结构",
        "category": "analyze"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == typescriptreact || resourceLangId == javascriptreact",
          "command": "reacooree.分析文件内的 hooks 结构",
          "group": "analyze"
        }
      ],
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "reacooree.分析组件内的 hooks 结构",
          "group": "analyze"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
